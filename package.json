{
  "name": "playwright-teams-demo",
  "version": "1.0.0",
  "description": "Playwright tests with Teams notifications",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "report": "npm run allure:generate && npm run allure:open",
    "codegen": "playwright codegen",
    "codegen:url": "playwright codegen https://version2-develop.fdm.dk/vi-tilbyder/forsikring/bilreparation"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^24.6.2",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.1",
    "playwright-msteams-reporter": "^0.0.13"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}